{
  "uid": "sor-overview",
  "title": "Silent Oak Ranch Overview",
  "tags": ["silent-oak"],
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Backend API Status Codes (5m rate)",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (code) (rate(traefik_service_requests_total{service=\"backend@docker\"}[5m]))",
          "legendFormat": "{{code}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "showLegend": true, "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Booking events / minute",
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sor_booking_events_last_minute",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "" },
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Contract queue delay (seconds)",
      "id": 3,
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sor_queue_oldest_seconds",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "" },
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 30 },
              { "color": "red", "value": 60 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Root filesystem usage",
      "id": 4,
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "(1 - (node_filesystem_avail_bytes{mountpoint=\"/\",fstype!~\"tmpfs|overlay\"} / node_filesystem_size_bytes{mountpoint=\"/\",fstype!~\"tmpfs|overlay\"})) * 100",
          "legendFormat": "Usage %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": false }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    }
  ],
  "time": { "from": "now-6h", "to": "now" },
  "templating": { "list": [] }
}
